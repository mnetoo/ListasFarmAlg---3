program exe051;

var

    n, i, numero, cubo, soma: longint;
    
begin

    read(n);
    i:= 1;
    numero:= 1;
    
        while (i <= n) do
        begin
        
            cubo:= numero * numero * numero;
            numero:= numero + 1; 
            soma:= soma + cubo;
            i:= i + 1;
            
        end;
        
    if (n = 0) then
        write('erro');
        
    write(soma);
        
end.
