program exe047;

var

    i, soma, numero, quadrado: longint;
    
begin

    i:= 0;
    soma:= 0;
    numero:= 1;
    
        while i < 50 do
        begin
        
            quadrado:= numero * numero;
            soma:= soma + quadrado;
            numero:= numero + 1;
            i:= i + 1;
            
        end;
        
    write(soma);
    
end.
            
